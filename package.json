{
  "name": "wisdoms-app",
  "version": "1.0.0",
  "description": "Wisdoms App",
  "type": "module",
  "scripts": {
    "prepare": "husky || true",
    "preinstall": "npx only-allow pnpm",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache --fix",
    "format": "prettier --write --cache --ignore-unknown .",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "wisdoms:post": "tsx scripts/postWisdom.ts",
    "wisdoms:post:test": "tsx --env-file .env scripts/postWisdom.ts"
  },
  "keywords": [
    "wisdoms",
    "bg",
    "en"
  ],
  "author": "Petar Zarkov",
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@types/node": "^24.9.2",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.3",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
