{
  "name": "wisdoms-app",
  "version": "1.0.0",
  "description": "An app for wisdoms",
  "keywords": [
    "node",
    "ts",
    "http",
    "koa",
    "wisdoms"
  ],
  "author": "Petar Zarkov",
  "license": "MIT",
  "main": "index.ts",
  "scripts": {
    "start": "node build/src/index",
    "start:env": "node -r dotenv/config build/src/index",
    "bns": "npm run build && npm run start",
    "bnw": "npm run build",
    "prebuild": "npm run clean",
    "clean": "tsc --build --clean",
    "build": "tsc -p tsconfig.json",
    "build:watch": "npm run build -- --watch",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "lint:fix": "npm run lint --fix",
    "db:create": "node .bin/exec ./migrations/db-wisdoms sequelize db:create",
    "db:migrate": "node .bin/exec ./migrations/db-wisdoms sequelize db:migrate",
    "db:seed": "node .bin/seedBuilder db-wisdoms && node .bin/exec ./migrations/db-wisdoms sequelize db:seed:all",
    "db:update": "npm run db:migrate && npm run db:seed"
  },
  "devDependencies": {
    "@types/jsonschema": "1.1.1",
    "@types/pg": "7.1.5",
    "@types/koa": "2.11.6",
    "@types/validator": "10.11.0",
    "@types/config": "0.0.37",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-router": "7.4.1",
    "@types/koa-send": "4.1.2",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "8.3.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@p.zarkov/hotstuff": "0.0.8",
    "config": "3.3.6",
    "dotenv": "8.2.0",
    "http-status-codes": "2.1.4",
    "jsonschema": "1.4.0",
    "koa": "2.13.1",
    "koa-bodyparser": "4.3.0",
    "koa-router": "10.0.0",
    "koa-send": "5.0.1",
    "node-fetch": "2.6.1",
    "pg": "8.6.0",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.6.2",
    "sequelize-cli": "6.2.0",
    "sequelize-typescript": "2.1.0",
    "ts-node": "9.1.1"
  }
}
